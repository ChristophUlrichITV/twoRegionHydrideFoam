FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     twoRegionHydrideFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1e0;

deltaT          1e-3;

writeControl    runTime;

writeInterval   1e-1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


libs (
    "libgroovyBC.so"
);

